<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:focusable="true"
              android:focusableInTouchMode="true"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:paddingLeft="@dimen/activity_horizontal_margin"
              android:paddingRight="@dimen/activity_horizontal_margin"
              android:paddingTop="@dimen/activity_vertical_margin"
              android:paddingBottom="@dimen/activity_vertical_margin"
              android:orientation="vertical"
              tools:context="ru.ifmo.md.lesson4.MainActivity">

    <TextView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/calcTextField"
            android:textSize="30sp"
            android:fontFamily="Open Sans"
            android:background="@drawable/enter_field_shape"
            android:scrollHorizontally="true"
            android:singleLine="true"
            android:layout_weight="5"
            android:inputType="none"
            android:layout_gravity="center_vertical" android:layout_margin="2dp" android:gravity="center_vertical|start"
            android:paddingLeft="10dp" android:paddingRight="10dp"
            android:ellipsize="start"/>
    <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp" android:gravity="center|bottom"
            android:baselineAligned="false" android:measureWithLargestChild="false"
            android:showDividers="beginning|middle|end"
            android:fontFamily="Open Sans"
            android:layout_weight="0.7">
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:measureWithLargestChild="false"
                android:layout_weight="1" android:id="@+id/tableRow1"
                >
            <Button
                    android:background="@drawable/button_custom"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="@string/button16"
                    android:textSize="30sp"
                    android:id="@+id/button16" android:layout_column="0" android:layout_weight="1"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"/>
            <Button
                    android:background="@drawable/button_custom"
                    android:layout_width="wrap_content"
                    android:onClick="onCalcButtonPress"
                    android:layout_height="fill_parent"
                    android:textSize="30sp" android:text="@string/button17"
                    android:id="@+id/button17" android:layout_column="1" android:layout_weight="1"
                    android:layout_margin="2dp"/>
            <Button
                    android:background="@drawable/button_custom"
                    android:layout_width="wrap_content"
                    android:layout_margin="2dp"
                    android:onClick="onCalcButtonPress"
                    android:layout_height="fill_parent"
                    android:textSize="30sp" android:text="@string/button18"
                    android:id="@+id/button18" android:layout_column="2" android:layout_weight="1"/>
            <Button
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:background="@drawable/button_custom"
                    android:layout_margin="2dp"
                    android:textSize="30dp" android:text="@string/button19"
                    android:onClick="onCalcButtonPress"
                    android:id="@+id/button19" android:layout_column="3" android:layout_weight="1"
                    />
        </TableRow>
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/tableRow2"
                >
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button12"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button12"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button13"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button13"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button14"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button14"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button15"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button15"
                    android:layout_weight="1"/>
        </TableRow>
        <TableRow android:layout_width="fill_parent"
                  android:layout_height="fill_parent"
                  android:layout_weight="1"
                  android:id="@+id/tableRow3">
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button8"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button8"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button9"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button9"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button10"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button10"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button11"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button11"
                    android:layout_weight="1"/>
        </TableRow>
        <TableRow android:layout_width="fill_parent"
                  android:layout_height="fill_parent"
                  android:layout_weight="1"
                  android:id="@+id/tableRow4">
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button4"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button4"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button5"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button5"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button6"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button6"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button7"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button7"
                    android:layout_weight="1"/>
        </TableRow>
        <TableRow android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_weight="1"
                  android:id="@+id/tableRow5">
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button0"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button0"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button1"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button1"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button2"
                    android:onClick="onCalcButtonPress"
                    android:longClickable="true"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_custom"
                    android:id="@+id/button2"
                    android:layout_weight="1"/>
            <Button android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:textSize="30dp" android:text="@string/button3"
                    android:onClick="onCalcButtonPress"
                    android:layout_margin="2dp"
                    android:background="@drawable/button_special"
                    android:id="@+id/button3"
                    android:layout_weight="1"/>
        </TableRow>
    </TableLayout>
</LinearLayout>
